					<handler function="OnZoomIn" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="ID_ZOOMOUT" variable="Menu5" member="yes">
					<label>Уменьшить масштаб</label>
					<handler function="OnZoomOut" entry="EVT_MENU" />
