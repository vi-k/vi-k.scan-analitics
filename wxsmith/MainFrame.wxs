					<handler function="OnSettings" entry="EVT_MENU" />
				</object>
				<object class="separator" />
				<object class="wxMenuItem" name="ID_MENU_QUIT" variable="MenuItem1" member="no">
					<label>Выход</label>
					<accel>Alt-F4</accel>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="yes">
				<label>Вид</label>
				<object class="wxMenu" name="ID_MENUITEM1" variable="MenuItem3" member="yes">
					<label>Карты</label>
					<object class="wxMenuItem" name="ID_MENUITEM2" variable="MenuItem4" member="yes">
